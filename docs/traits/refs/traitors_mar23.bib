	isbn = {0022-0477},
	issn = {00220477},
	journal = {Journal of Ecology},
	keywords = {Determinants of plant community diversity and stru,Height growth trajectory,Leaf mass per area,Life-history strategy,Maximum plant height,RGR,Stem tissue mass density},
	number = {4},
