	author = {Carter, Shannon K. and Saenz, Daniel and Rudolf, Volker H.W.},
	date-added = {2023-12-05 10:14:43 -0800},
	date-modified = {2023-12-05 10:14:43 -0800},
	doi = {10.1111/ele.13081},
	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Carter, Saenz, Rudolf/Ecology Letters/Carter, Saenz, Rudolf - 2018 - Shifts in phenological distributions reshape interaction potential in natural communities.pdf:pdf},
	issn = {14610248},
