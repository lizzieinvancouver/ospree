	language = {en},
	month = mar,
	number = {3},
	pages = {424--429},
	shorttitle = {Time -- size tradeoffs},
	title = {Time -- size tradeoffs: a phylogenetic comparative study of flowering time, plant height and seed mass in a north‐temperate flora},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/j.2007.0030-1299.16142.x},
