	issn = {0962-8452},
	journal = {Proceedings of the Royal Society B},
	keywords = {climate change,laying date,parus major,phenotypic plasticity,timing of reproduction},
	number = {May},
	pages = {1867--1870},
	pmid = {76410600009},
	title = {{Warmer springs lead to mistimes reproduction in Great Tits (Parus major)}},
