	doi = {10.1016/j.actao.2021.103760},
	file = {Wang et al. - 2021 - Differences in leaf phenological traits between tr.pdf:/Users/deirdreloughnan/Zotero/storage/LAB3A9KH/Wang et al. - 2021 - Differences in leaf phenological traits between tr.pdf:application/pdf},
	issn = {1146609X},
	journal = {Acta Oecologica},
	language = {en},
	month = oct,
	pages = {103760},
	title = {Differences in leaf phenological traits between trees and shrubs are closely related to functional traits in a temperate forest},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1146609X2100059X},
