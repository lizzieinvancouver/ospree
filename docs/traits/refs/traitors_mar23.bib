	journal = {Ecology Letters},
	language = {en},
	month = aug,
	number = {8},
	pages = {1324--1338},
	title = {The role of seasonal timing and phenological shifts for species coexistence},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/ele.13277},
