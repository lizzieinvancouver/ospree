	issn = {2352-3409},
	journal = {Data in Brief},
	keywords = {Species range, Botany, Tree, Shrub, Distribution},
	pages = {662-666},
	title = {{Chorological maps for the main European woody species}},
	url = {https://www.sciencedirect.com/science/article/pii/S2352340917301981},
