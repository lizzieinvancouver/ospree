	journal = {Plant, Cell \& Environment},
	keywords = {budburst models, dates of flowering, external validity, local adaptation of phenology to climate},
	number = {1},
	pages = {1-13},
	title = {Selecting models to predict the timing of flowering of temperate trees: implications for tree phenology modelling},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1046/j.1365-3040.1999.00395.x},
